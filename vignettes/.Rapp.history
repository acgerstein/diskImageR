compLik <- function(ML1, ML2){	#
	numPar <- c()#
	diff <- c()#
#
	for (i in 1:length(ML1)){#
	diff[i] <- ML2[[i]]$lnLik	-ML1[[i]]$lnLik#
		if (((ML2[[i]]$lnLik - ML1[[i]]$lnLik)*2) > qchisq(0.95, 3)) {numPar[i] <- "2par"}#
		else {numPar[i] <- "1par"}	#
		}#
	return(list(diff, numPar))#
	}
